{
  "name": "express-zod-enhancer",
  "version": "1.0.0",
  "description": "Enhance your express routes using Zod schemas",
  "homepage": "https://github.com/kumpmati/express-zod-enhancer",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "Matias Kumpulainen",
  "license": "ISC",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "tsc-watch --onSuccess \"node dist/src\"",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "package": "tsup lib/index.ts --format esm,cjs --clean --sourcemap --dts --out-dir dist"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.5.1",
    "tsc-watch": "^4.6.0",
    "tsup": "^7.1.0",
    "typescript": "^4.5.5",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "openapi3-ts": "^4.1.2",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "zod": "^3.21.4",
    "zod-fixture": "^1.10.0",
    "zod-to-json-schema": "^3.21.3"
  },
  "peerDependencies": {
    "express": "^4.17.2"
  }
}
