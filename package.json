{
  "name": "ts-express-boilerplate",
  "version": "1.0.0",
  "description": "Express application skeleton with TypeScript",
  "main": "index.js",
  "keywords": [
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "Matias Kumpulainen",
  "license": "ISC",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "tsc-watch --onSuccess \"node dist/src\"",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint ."
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.5.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-list-endpoints": "^6.0.0",
    "openapi3-ts": "^4.1.2",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "redoc-express": "^2.1.0",
    "zod": "^3.21.4",
    "zod-fixture": "^1.10.0",
    "zod-to-json-schema": "^3.21.3"
  }
}
